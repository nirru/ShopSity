<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_horizontal_margin"
    tools:context="com.example.nm.mobikyte.DashboardFragment">


    <RelativeLayout
        android:id="@+id/RelativeI"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentLeft="true">
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="11200"
                android:layout_gravity="left"/>
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="CompaignId"
                android:layout_gravity="left"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_centerInParent="true">
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="11200"
                android:layout_gravity="center"/>
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_gravity="center"
                android:text="CompaignId" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentRight="true">
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="11200"
                android:layout_gravity="right"/>
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_gravity="right"
                android:text="CompaignId" />
        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeII"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/RelativeI"
        android:layout_marginTop="10dp">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentLeft="true">
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="11200"
                android:layout_gravity="left"/>
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="CompaignId"
                android:layout_gravity="left"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_centerInParent="true">
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="11200"
                android:layout_gravity="center"/>
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_gravity="center"
                android:text="CompaignId" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentRight="true">
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="11200"
                android:layout_gravity="right"/>
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_gravity="right"
                android:text="CompaignId" />
        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeIII"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/RelativeII"
        android:layout_marginTop="20dp">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentLeft="true">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/logo"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_centerInParent="true">
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Promotion Preview"
                android:layout_gravity="top"/>
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginTop="10dp"
                android:text="Promotion Preview" />
        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeIV"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/RelativeIII"
        android:layout_marginTop="20dp">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentRight="true">
            <in.championswimmer.libsocialbuttons.buttons.BtnFacebook
                android:text="Follow on Facebook"
                android:layout_width="wrap_content"
                android:textSize="12sp"
                android:padding="5dp"
                android:gravity="center"
                android:layout_height="wrap_content" />
        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeV"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/RelativeIV"
        android:layout_marginTop="20dp">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentLeft="true">
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="11200"
                android:layout_gravity="left"/>
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="CompaignId"
                android:layout_gravity="left"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_centerInParent="true">
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="11200"
                android:layout_gravity="center"/>
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_gravity="center"
                android:text="CompaignId" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentRight="true">
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="11200"
                android:layout_gravity="right"/>
            <com.oxilo.shopsity.ui.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_gravity="right"
                android:text="CompaignId" />
        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeVI"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/RelativeV"
        android:layout_marginTop="20dp">

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/action_Impression"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Impressions and Click Graphs"
            android:textAllCaps="false"
            android:layout_gravity="center_horizontal" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeVII"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/RelativeVI"
        android:layout_marginTop="10dp">

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/action_Location"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Location Heat Map"
            android:textAllCaps="false"
            android:layout_gravity="center_horizontal" />

    </RelativeLayout>

</RelativeLayout>