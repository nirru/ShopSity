<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cardview="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

   <!--<android.support.v7.widget.CardView-->
       <!--android:layout_width="fill_parent"-->
       <!--android:layout_height="wrap_content"-->
       <!--android:layout_gravity="center"-->
       <!--cardview:cardElevation="5dp"-->
       <!--cardview:cardCornerRadius="5dp">-->
       <RelativeLayout
           android:layout_width="fill_parent"
           android:layout_height="wrap_content"
           android:orientation="vertical"
           android:padding="8dp">

           <RelativeLayout
               android:id="@+id/RelativeLayout01"
               android:layout_width="fill_parent"
               android:gravity="center"
               android:layout_height="30dp">
               <com.oxilo.shopsity.ui.CustomTextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:textColor="@android:color/black"
                   android:text="@string/campaign_id"
                   android:layout_alignParentLeft="true"/>
               <com.oxilo.shopsity.ui.CustomTextView
                   android:id="@+id/action_campaign_id"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:textColor="@color/colorPrimaryDark"
                   android:layout_alignParentRight="true"
                   android:text="us@ussgdh"/>
               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="1dp"
                   android:layout_below="@+id/action_campaign_id"
                   android:background="@android:color/darker_gray"/>
           </RelativeLayout>

           <RelativeLayout
               android:id="@+id/RelativeLayout02"
               android:layout_width="fill_parent"
               android:gravity="center"
               android:layout_below="@+id/RelativeLayout01"
               android:layout_height="30dp">
               <com.oxilo.shopsity.ui.CustomTextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="@string/start_time"
                   android:textColor="@android:color/black"
                   android:layout_alignParentLeft="true"/>
               <com.oxilo.shopsity.ui.CustomTextView
                   android:id="@+id/action_start_time"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:textColor="@color/colorPrimaryDark"
                   android:layout_alignParentRight="true"
                   android:text="us@ussgdh"/>

               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="1dp"
                   android:layout_below="@+id/action_start_time"
                   android:background="@android:color/darker_gray"/>
           </RelativeLayout>

           <RelativeLayout
               android:id="@+id/RelativeLayout03"
               android:layout_width="fill_parent"
               android:layout_below="@+id/RelativeLayout02"
               android:gravity="center"
               android:layout_height="30dp">
               <com.oxilo.shopsity.ui.CustomTextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="@string/total_ad"
                   android:textColor="@android:color/black"
                   android:layout_alignParentLeft="true"/>
               <com.oxilo.shopsity.ui.CustomTextView
                   android:id="@+id/action_total_ads"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:textColor="@color/colorPrimaryDark"
                   android:layout_alignParentRight="true"
                   android:text="us@ussgdh"/>

               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="1dp"
                   android:layout_below="@+id/action_total_ads"
                   android:background="@android:color/darker_gray"/>
           </RelativeLayout>

           <RelativeLayout
               android:id="@+id/RelativeLayout04"
               android:layout_width="fill_parent"
               android:layout_below="@+id/RelativeLayout03"
               android:gravity="center"
               android:layout_height="30dp">
               <com.oxilo.shopsity.ui.CustomTextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="@string/total_ad_remaining"
                   android:textColor="@android:color/black"
                   android:layout_alignParentLeft="true"/>
               <com.oxilo.shopsity.ui.CustomTextView
                   android:id="@+id/action_total_ads_remaining"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:textColor="@color/colorPrimaryDark"
                   android:layout_alignParentRight="true"
                   android:text="us@ussgdh"/>

               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="1dp"
                   android:layout_below="@+id/action_total_ads_remaining"
                   android:background="@android:color/darker_gray"/>
           </RelativeLayout>

           <RelativeLayout
               android:id="@+id/RelativeLayout05"
               android:layout_width="fill_parent"
               android:layout_below="@+id/RelativeLayout04"
               android:gravity="center"
               android:layout_height="30dp">
               <com.oxilo.shopsity.ui.CustomTextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="@string/call_web"
                   android:textColor="@android:color/black"
                   android:layout_alignParentLeft="true"/>
               <com.oxilo.shopsity.ui.CustomTextView
                   android:id="@+id/action_call_web"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:textColor="@color/colorPrimaryDark"
                   android:layout_alignParentRight="true"
                   android:text="us@ussgdh"/>

               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="1dp"
                   android:layout_below="@+id/action_call_web"
                   android:background="@android:color/darker_gray"/>
           </RelativeLayout>

           <RelativeLayout
               android:id="@+id/RelativeLayout06"
               android:layout_width="fill_parent"
               android:layout_below="@+id/RelativeLayout05"
               android:gravity="center"
               android:layout_height="30dp">
               <com.oxilo.shopsity.ui.CustomTextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="@string/status"
                   android:textColor="@android:color/black"
                   android:layout_alignParentLeft="true"/>
               <com.oxilo.shopsity.ui.CustomTextView
                   android:id="@+id/action_status"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:textColor="@color/colorPrimaryDark"
                   android:layout_alignParentRight="true"
                   android:text="@string/upload"/>

               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="1dp"
                   android:layout_below="@+id/action_status"
                   android:background="@android:color/darker_gray"/>
           </RelativeLayout>


           <TextView
               android:id="@+id/action_dashboard"
               android:layout_width="125dp"
               android:layout_height="wrap_content"
               android:padding="5dp"
               android:gravity="center"
               android:textAppearance="?android:textAppearanceSmall"
               android:background="@color/colorPrimaryDark"
               android:layout_below="@+id/RelativeLayout06"
               android:layout_alignParentRight="true"
               android:textAllCaps="false"
               android:text="Dashboard" />


       </RelativeLayout>
   <!--</android.support.v7.widget.CardView>-->
</LinearLayout>