<com.desarrollodroide.libraryfragmenttransactionextended.SlidingRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--<android.support.v7.widget.Toolbar-->
        <!--android:id="@+id/toolbar"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="?attr/actionBarSize"-->
        <!--android:background="@color/colorPrimary"-->
        <!--android:minHeight="?attr/actionBarSize"-->
        <!--app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"-->
        <!--app:popupTheme="@style/ThemeOverlay.AppCompat.Dark.ActionBar" />-->
<android.support.v4.widget.NestedScrollView
android:layout_width="match_parent"
android:layout_height="match_parent"
android:padding="16dp">
<RelativeLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:id="@+id/RelativeLayout01"
        style="@style/set_relat">
        <com.oxilo.shopsity.ui.CustomTextView
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentLeft="true"/>
        <com.oxilo.shopsity.ui.CustomTextView
            android:id="@+id/action_view_name"
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentRight="true"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/action_view_name"
            android:background="@android:color/darker_gray"></View>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/RelativeLayout02"
        android:layout_below="@id/RelativeLayout01"
        style="@style/set_relat">

        <com.oxilo.shopsity.ui.CustomTextView
            style="@style/msgtextbold"
            android:text="Business Name"
            android:layout_alignParentLeft="true"/>

        <com.oxilo.shopsity.ui.CustomTextView
            android:id="@+id/action_bussiness_name"
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentRight="true"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/action_bussiness_name"
            android:background="@android:color/darker_gray"></View>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/RelativeLayout03"
        android:layout_below="@+id/RelativeLayout02"
        style="@style/set_relat">

        <com.oxilo.shopsity.ui.CustomTextView
            style="@style/msgtextbold"
            android:text="UserName"
            android:layout_alignParentLeft="true"/>

        <com.oxilo.shopsity.ui.CustomTextView
            android:id="@+id/action_user_name"
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentRight="true"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/action_user_name"
            android:background="@android:color/darker_gray"></View>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/RelativeLayout04"
        style="@style/set_relat"
        android:layout_below="@+id/RelativeLayout03">

        <com.oxilo.shopsity.ui.CustomTextView
            style="@style/msgtextbold"
            android:text="Business Address"
            android:layout_alignParentLeft="true"/>

        <com.oxilo.shopsity.ui.CustomTextView
            android:id="@+id/action_bussiness_address"
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentRight="true"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/action_bussiness_address"
            android:background="@android:color/darker_gray"></View>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/RelativeLayout05"
        style="@style/set_relat"
        android:layout_below="@+id/RelativeLayout04">

        <com.oxilo.shopsity.ui.CustomTextView
            style="@style/msgtextbold"
            android:text="City"
            android:layout_alignParentLeft="true"/>

        <com.oxilo.shopsity.ui.CustomTextView
            android:id="@+id/action_user_city"
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentRight="true"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/action_user_city"
            android:background="@android:color/darker_gray"></View>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/RelativeLayout06"
        style="@style/set_relat"
        android:layout_below="@+id/RelativeLayout05">

        <com.oxilo.shopsity.ui.CustomTextView
            style="@style/msgtextbold"
            android:text="state"
            android:layout_alignParentLeft="true"/>

        <com.oxilo.shopsity.ui.CustomTextView
            android:id="@+id/action_user_state"
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentRight="true"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/action_user_state"
            android:background="@android:color/darker_gray"></View>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/RelativeLayout07"
        style="@style/set_relat"
        android:layout_below="@+id/RelativeLayout06">

        <com.oxilo.shopsity.ui.CustomTextView
            style="@style/msgtextbold"
            android:text="Zip Code"
            android:layout_alignParentLeft="true"/>
        <com.oxilo.shopsity.ui.CustomTextView
            android:id="@+id/action_zip_code"
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentRight="true"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/action_zip_code"
            android:background="@android:color/darker_gray"></View>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/RelativeLayout08"
        style="@style/set_relat"
        android:layout_below="@+id/RelativeLayout07">

        <com.oxilo.shopsity.ui.CustomTextView
            style="@style/msgtextbold"
            android:text="Email"
            android:layout_alignParentLeft="true"/>

        <com.oxilo.shopsity.ui.CustomTextView
            android:id="@+id/action_email"
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentRight="true"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/action_email"
            android:background="@android:color/darker_gray"></View>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/RelativeLayout09"
        style="@style/set_relat"
        android:layout_below="@+id/RelativeLayout08">

        <com.oxilo.shopsity.ui.CustomTextView
            style="@style/msgtextbold"
            android:text="Your website's url"
            android:layout_alignParentLeft="true"/>
        <com.oxilo.shopsity.ui.CustomTextView
            android:id="@+id/action_web_url"
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentRight="true"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/action_web_url"
            android:background="@android:color/darker_gray"></View>


    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/RelativeLayout10"
        style="@style/set_relat"
        android:layout_below="@+id/RelativeLayout09">

        <com.oxilo.shopsity.ui.CustomTextView
            style="@style/msgtextbold"
            android:text="Mobile Number"
            android:layout_alignParentLeft="true"/>
        <com.oxilo.shopsity.ui.CustomTextView
            android:id="@+id/action_mobile_no"
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentRight="true"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/action_mobile_no"
            android:background="@android:color/darker_gray"></View>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/RelativeLayout11"
        style="@style/set_relat"
        android:layout_below="@+id/RelativeLayout10">

        <com.oxilo.shopsity.ui.CustomTextView
            style="@style/msgtextbold"
            android:text="About"
            android:layout_alignParentLeft="true"/>

        <com.oxilo.shopsity.ui.CustomTextView
            android:id="@+id/action_about"
            style="@style/msgtextbold"
            android:text="Name"
            android:layout_alignParentRight="true"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/action_about"
            android:background="@android:color/darker_gray"></View>
    </RelativeLayout>


</RelativeLayout>
</android.support.v4.widget.NestedScrollView>
    </com.desarrollodroide.libraryfragmenttransactionextended.SlidingRelativeLayout>
